{
    "openapi": "3.0.0",
    "info": {
      "title": "API de Autenticação de Contratantes e Prestadores",
      "description": "API para cadastro e autenticação de contratantes e prestadores de serviços.",
      "version": "1.0.0"
    },
    "servers": [
      {
        "url": "http://localhost:3000",
        "description": "Servidor local"
      }
    ],
    "paths": {
      "/login/contratantes": {
        "post": {
          "summary": "Login de contratante",
          "tags": ["Autenticação Contratantes"],
          "requestBody": {
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "email": {
                      "type": "string",
                      "example": "contratante@example.com"
                    },
                    "senha": {
                      "type": "string",
                      "example": "senha123"
                    }
                  }
                }
              }
            }
          },
          "responses": {
            "200": {
              "description": "Login realizado com sucesso",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "acessToken": {
                        "type": "string",
                        "example": "token_jwt_aqui"
                      }
                    }
                  }
                }
              }
            },
            "401": {
              "description": "Usuário ou senha inválido"
            }
          }
        }
      },
      "/cadastro/contratantes": {
        "post": {
          "summary": "Cadastro de contratante",
          "tags": ["Autenticação Contratantes"],
          "requestBody": {
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "nome": {
                      "type": "string",
                      "example": "João Silva"
                    },
                    "email": {
                      "type": "string",
                      "example": "contratante@example.com"
                    },
                    "senha": {
                      "type": "string",
                      "example": "senha123"
                    },
                    "uf": {
                      "type": "string",
                      "example": "SP"
                    },
                    "cidade": {
                      "type": "string",
                      "example": "São Paulo"
                    },
                    "cep": {
                      "type": "string",
                      "example": "01000-000"
                    },
                    "rua": {
                      "type": "string",
                      "example": "Rua Exemplo"
                    },
                    "numero": {
                      "type": "string",
                      "example": "123"
                    },
                    "complemento": {
                      "type": "string",
                      "example": "Apto 101"
                    }
                  }
                }
              }
            }
          },
          "responses": {
            "201": {
              "description": "Contratante cadastrado com sucesso"
            },
            "400": {
              "description": "Erro no cadastro"
            }
          }
        }
      },
      "/login/prestadores": {
        "post": {
          "summary": "Login de prestador",
          "tags": ["Autenticação Prestadores"],
          "requestBody": {
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "email": {
                      "type": "string",
                      "example": "prestador@example.com"
                    },
                    "senha": {
                      "type": "string",
                      "example": "senha123"
                    }
                  }
                }
              }
            }
          },
          "responses": {
            "200": {
              "description": "Login realizado com sucesso",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "acessToken": {
                        "type": "string",
                        "example": "token_jwt_aqui"
                      }
                    }
                  }
                }
              }
            },
            "401": {
              "description": "Usuário ou senha inválido"
            }
          }
        }
      },
      "/cadastro/prestadores": {
        "post": {
          "summary": "Cadastro de prestador",
          "tags": ["Autenticação Prestadores"],
          "requestBody": {
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "nome": {
                      "type": "string",
                      "example": "Maria Souza"
                    },
                    "email": {
                      "type": "string",
                      "example": "prestador@example.com"
                    },
                    "senha": {
                      "type": "string",
                      "example": "senha123"
                    },
                    "uf": {
                      "type": "string",
                      "example": "RJ"
                    },
                    "cidade": {
                      "type": "string",
                      "example": "Rio de Janeiro"
                    },
                    "cep": {
                      "type": "string",
                      "example": "20000-000"
                    },
                    "rua": {
                      "type": "string",
                      "example": "Rua Prestador"
                    },
                    "numero": {
                      "type": "string",
                      "example": "456"
                    },
                    "complemento": {
                      "type": "string",
                      "example": "Casa 2"
                    }
                  }
                }
              }
            }
          },
          "responses": {
            "201": {
              "description": "Prestador cadastrado com sucesso"
            },
            "400": {
              "description": "Erro no cadastro"
            }
          }
        }
      }
    },
    "components": {
      "securitySchemes": {
        "bearerAuth": {
          "type": "http",
          "scheme": "bearer",
          "bearerFormat": "JWT"
        }
      }
    }
  }
  